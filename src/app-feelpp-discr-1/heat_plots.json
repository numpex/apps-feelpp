            "color_axis":{
                "parameter":"mesh",
                "label":"Mesh"
            },
            "secondary_axis":{
                "parameter":"discretization",
                "label":"Discretization"
            },
            "aggregations":[
                {"column":"performance_variable","agg":"filter:Solve_ksp-niter"}
