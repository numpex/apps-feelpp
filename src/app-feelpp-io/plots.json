{
  "plots": [
    {
      "title": "Time to load Mesh",
      "plot_types": ["scatter"],
      "transformation": "performance",
      "variables": ["timeMeasures_loadMesh"],
      "names": ["ksp"],
      "xaxis":{
        "parameter": "resources.tasks",
        "label": "Number of tasks"
      },
      "yaxis":{
        "label": "Number of iterations"
      },
      "color_axis":{
        "parameter": "mesh",
        "label": "Mesh"
      },
      "secondary_axis":{
        "parameter": "discretization",
        "label": "Discretization"
      },
      "aggregations": [
        {"column": "performance_variable","agg": "filter:timeMeasures_loadMesh"}
      ]
    },
